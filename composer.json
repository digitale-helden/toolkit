  "type": "library",
  "keywords": [
    "php",
    "toolkit",
    "functions",
    "helper"
  ],
  "license": "MIT",
  "authors": [{
    "name": "setcooki",
    "email": "set@cooki.me",
    "homepage": "http://set.cooki.me"
  }],
  "support": {
    "issues": "https://github.com/digitale-helden/toolkit/issues",
    "wiki": "https://github.com/digitale-helden/toolkit/wiki",
    "source": "https://github.com/digitale-helden/toolkit"
  },
  "config": {
    "vendor-dir": "lib/vendor"
  },
  "require": {
    "php": ">=5.6.1"
  },
  "require-dev": {
    "apigen/apigen": "4.1.*"
  },
  "autoload": {
    "src": {
      "psr-4": {
        "DigitaleHelden\\": "src/"
      }
    },
    "files": [
